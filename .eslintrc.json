{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "airbnb/hooks", "next/core-web-vitals"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "arrow-body-style": ["error", "always"],
        "no-underscore-dangle": [
          "error",
          {
            "allow": ["_id"]
          }
        ]
      },
}
